#include<bits/stdc++.h>
using namespace std;
#define tt int t;cin>>t;while(t--)

#define yes cout<<"YES"<<endl
#define no cout<<"NO"<<endl
#define ll long long
#define end cout<<endl
int main(){
  ios::sync_with_stdio(false);
    cin.tie(nullptr);
tt{
    
string s;
cin>>s;
for(int i=0;i<s.length()-1;i++){
    if(s[i]=='u'&&s[i+1]=='s'){
        cout<<'i';
        i++;
    }
    else cout<<s[i];

}
cout<<endl;
}
}