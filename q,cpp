#include<bits/stdc++.h>
using namespace std;
#define tt int t;cin>>t;while(t--)

#define yes cout<<"YES"<<endl
#define no cout<<"NO"<<endl
#define ll long long
#define end cout<<endl
int main(){
  ios::sync_with_stdio(false);
    cin.tie(nullptr);
tt{
    int n,m,k;
    cin>>n>>m>>k;
    int arr[n*m];
    for(auto &it:arr) cin>>it;
    if(k>m) cout<<m<<endl;
    else {
        int freq[n]={0};
        int sum=m-k;
        for(int i=0;i<n*m;i++){
            freq[arr[i]/m]++;
            if(freq[arr[i]/m]==sum) {
                cout<<i+1<<endl;
                break;
            }
        }
    }

}
}